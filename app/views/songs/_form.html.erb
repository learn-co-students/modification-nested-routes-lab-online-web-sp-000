<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= @song.artist.name if @song.artist && @path == "nested" %>
  <%= select_tag "song[artist_id]", options_from_collection_for_select(Artist.all, :id, :name) %>
  <%= f.submit %>
<% end %>
